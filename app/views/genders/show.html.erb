<div class="page-header">
  <h3>Gender #<%= @gender.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Gender</dt>
      <dd><%= @gender.gender %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/genders" class="btn btn-primary">
        Back
      </a>
      <a href="/genders/<%= @gender.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_gender/<%= @gender.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
