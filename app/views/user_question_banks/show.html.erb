<div class="page-header">
  <h3>User Question Bank #<%= @user_question_bank.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @user_question_bank.user_id %></dd>

      <dt>Question</dt>
      <dd><%= @user_question_bank.question_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/user_question_banks" class="btn btn-primary">
        Back
      </a>
      <a href="/user_question_banks/<%= @user_question_bank.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_user_question_bank/<%= @user_question_bank.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
