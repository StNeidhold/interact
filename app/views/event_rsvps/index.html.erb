<div class="page-header">
  <h1>
    Event rsvps
    <a href="/event_rsvps/new" class="btn btn-lg btn-success">New Event rsvp</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Recruiting event</th>
        <th>User</th>
        <th>Actions</th>
      </tr>

      <% @event_rsvps.each do |event_rsvp| %>
      <tr>
        <td><%= event_rsvp.recruiting_event_id %></td>
        <td><%= event_rsvp.user_id %></td>
        <td>
          <a href="/event_rsvps/<%= event_rsvp.id %>" class="btn btn-primary">Show</a>
          <a href="/event_rsvps/<%= event_rsvp.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_event_rsvp/<%= event_rsvp.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
