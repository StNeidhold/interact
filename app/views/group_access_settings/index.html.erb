<div class="page-header">
  <h1>
    Group access settings
    <a href="/group_access_settings/new" class="btn btn-lg btn-success">New Group access setting</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Acessibility</th>
        <th>Visibility</th>
        <th>Actions</th>
      </tr>

      <% @group_access_settings.each do |group_access_setting| %>
      <tr>
        <td><%= group_access_setting.acessibility %></td>
        <td><%= group_access_setting.visibility %></td>
        <td>
          <a href="/group_access_settings/<%= group_access_setting.id %>" class="btn btn-primary">Show</a>
          <a href="/group_access_settings/<%= group_access_setting.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_group_access_setting/<%= group_access_setting.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @group_access_settings, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
