<div class="page-header">
  <h1>
    Process types
    <a href="/process_types/new" class="btn btn-lg btn-success">New Process type</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Process type</th>
        <th>High end</th>
        <th>Low end</th>
        <th>Actions</th>
      </tr>

      <% @process_types.each do |process_type| %>
      <tr>
        <td><%= process_type.process_type %></td>
        <td><%= process_type.high_end %></td>
        <td><%= process_type.low_end %></td>
        <td>
          <a href="/process_types/<%= process_type.id %>" class="btn btn-primary">Show</a>
          <a href="/process_types/<%= process_type.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_process_type/<%= process_type.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @process_types, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
