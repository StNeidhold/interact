<div class="page-header">
  <h1>
    User recruiting cycles
    <a href="/user_recruiting_cycles/new" class="btn btn-lg btn-success">New User recruiting cycle</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
        <th>Start date</th>
        <th>End date</th>
        <th>Name</th>
        <th>Is active</th>
        <th>Actions</th>
      </tr>

      <% @user_recruiting_cycles.each do |user_recruiting_cycle| %>
      <tr>
        <td><%= user_recruiting_cycle.user_id %></td>
        <td><%= user_recruiting_cycle.start_date %></td>
        <td><%= user_recruiting_cycle.end_date %></td>
        <td><%= user_recruiting_cycle.name %></td>
        <td><%= user_recruiting_cycle.is_active %></td>
        <td>
          <a href="/user_recruiting_cycles/<%= user_recruiting_cycle.id %>" class="btn btn-primary">Show</a>
          <a href="/user_recruiting_cycles/<%= user_recruiting_cycle.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_user_recruiting_cycle/<%= user_recruiting_cycle.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
